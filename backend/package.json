{
  "name": "metresurmesure-backend",
  "version": "1.0.0",
  "description": "Backend for metresurmesure application",
  "main": "server.js",
  "scripts": {
    "start": "node -e \"const port = process.env.PORT || 5000; require('net').createServer().listen(port, () => { console.log('Port', port, 'is available'); process.exit(0); }).on('error', (err) => { console.error('Port', port, 'is in use'); process.exit(1); });\" && node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "echo 'No build needed for Node.js'",
    "render-postbuild": "npm run migrate",
    "render-build": "npm install"
  },
  "dependencies": {
    "stripe": "^12.18.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.6.1",
    "nodemailer": "^6.9.1",
    "dotenv": "^16.0.3",
    "express-validator": "^6.15.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}